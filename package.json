    "@turbo/gen": "^1.10.14",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "concurrently": "^8.2.1",
    "eslint": "^8.50.0",
    "eslint-config-custom": "*",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prettier-config-custom": "*",
    "rimraf": "^5.0.1",
    "turbo": "^1.10.14",
    "typescript": "^5.2.2"
