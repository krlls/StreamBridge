{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite --host\" \"yarn storybook\"",
    "build": "tsc && vite build",
    "prettier": "prettier --write src",
    "lint-fix": "eslint --fix src",
    "fix": "yarn prettier && yarn lint-fix",
    "start": "vite preview",
    "storybook": "storybook dev -p 6006 --no-open"
